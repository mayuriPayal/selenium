package practice1;

public class methods1 {
	
void greeting ()
	
	{
		System.out.println("hello");
	}
	
	String greeting1 ()
	{
		return "how are u";
		//System.out.println("are");
	}

	void greeting2 (int a)
	{
		System.out.println(a);
	}
	
	void greeting3 (String aaaaa)
	{
		System.out.println("name"+ aaaaa);
	}
	
	String greeting4 (String name)
	{
		return ("hi");
	}
	
}
